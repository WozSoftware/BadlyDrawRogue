<?xml version="1.0" encoding="utf-8" ?>

<Entities>
  <Entity Type="Floor" Name="Dungeon Floor"/>
  
  <Entity Type="Wall" Name="Dungeon Wall">
    <Flags>
      <Flag Type="BlocksMovement" Value="true"/>
      <Flag Type="BlocksLineOfSight" Value="true"/>
    </Flags>
  </Entity>

  <Entity Type="Door" Name="Door">
    <Flags>
      <Flag Type="IsOpen" Value="false"/>
      <Flag Type="BlocksMovement" Value="true"/>
      <Flag Type="BlocksLineOfSight" Value="true"/>
    </Flags>
  </Entity>

  <Entity Type="Window" Name="Window">
    <Flags>
      <Flag Type="BlocksMovement" Value="true"/>
      <Flag Type="BlocksLineOfSight" Value="false"/>
    </Flags>
  </Entity>
</Entities>

